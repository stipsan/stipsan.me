{
  "name": "stipsan.me",
  "private": true,
  "description": "My own personal website, don't really need one but it's a good excuse to try new tools and experiment. Maybe others learn a thing or two by keeping the source public :-)",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "dev": "node server.js",
    "start": "node server.js",
    "eslint:ci": "eslint . --format junit --output-file .reports/eslint.xml",
    "eslint:workaround": "eslint . --format stylish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "now": {
    "public": true,
    "alias": "stipsan.me",
    "env": {
      "NODE_ENV": "production",
      "TRACKING_ID": "@tracking-id"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/stipsan.me.git"
  },
  "author": "Stian Didriksen <stipsan@gmail.com> (https://stipsan.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/stipsan.me/issues"
  },
  "homepage": "https://github.com/stipsan/stipsan.me#readme",
  "dependencies": {
    "babel-plugin-inline-react-svg": "0.4.0",
    "babel-plugin-styled-components": "1.1.7",
    "babel-plugin-transform-define": "1.3.0",
    "express": "4.15.3",
    "faker": "4.1.0",
    "grid-styled": "2.0.0-9",
    "isomorphic-fetch": "2.2.1",
    "lru-cache": "4.1.1",
    "md5-o-matic": "0.1.1",
    "next": "3.0.1-beta.18",
    "polished": "1.3.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-ga": "2.2.0",
    "react-helmet": "5.1.3",
    "react-markdown": "2.5.0",
    "react-no-ssr": "1.1.0",
    "styled-components": "2.1.1",
    "system-font-stack": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.3.0",
    "eslint-config-common-sense": "0.0.3",
    "eslint-plugin-react": "7.1.0"
  }
}
